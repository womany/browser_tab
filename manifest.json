{
	"name": "Womany-tab",
	"description": "在瀏覽器首頁上偷看 womany ！",
	"version": "0.0.1",
	"incognito": "split",
	"author": "Womany",

	"icons": {
		"48": "images/logo.png"
	},

	"permissions": [
		"*://api.womany.net/*",
		"http://google.com.tw/complete/*"
	],

	"chrome_url_overrides": {
		"newtab": "index.html"
	},

	"browser_action": {
	},

	"background": {
		"scripts": ["background.js"]
	},

	"manifest_version": 2
}
