{
  "manifest_version": 2,
  "name": "womany_tab",
  "version": "1.0",

  "description": "show random quote and hot articles when opening a new tab.",

  "icons": {
    "48": "icons/border-48.png"
  },

  "chrome_url_overrides" : {
    "newtab": "quote.html"
  },

  "permissions": [
    "https://api.womany.net/",
    "storage"
  ]

}
